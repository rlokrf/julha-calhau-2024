#include <iostream>
using namespace std;

int main()
{
    float grade[5], sum=0;
    int cont;

    for(cont=0 ; cont<5 ; cont++){
        cout<<"Insira um valor: "<<"";
        cin >> grade[cont];
        sum += grade[cont];
    }

    for(cont=0 ; cont<5 ; cont++)
        cout<<""<<""<<grade[cont]<<endl;

    sum += 0;

    cout<<"Soma Total: "<<sum<<endl;

}
