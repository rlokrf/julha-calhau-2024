#include <iostream>

int main() {
    const int size = 10;
    float values[size];

    std::cout << "Digite 10 valores reais:" << std::endl;
    for (int i = 0; i < size; i++) {
        std::cin >> values[i];
    }

    std::cout << "Valores na ordem inversa:" << std::endl;
    for (int i = size - 1; i >= 0; i--) {
        std::cout << values[i] << std::endl;
    }

    return 0;
}
