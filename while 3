#include <iostream>

int main() {
    int n;

    // Solicita ao usuário para inserir um número
    std::cout << "Digite um numero para ver sua tabuada: ";
    std::cin >> n;

    // Mostra a tabuada do número inserido pelo usuário
    std::cout << "Tabuada de " << n << ":" << std::endl;
    for (int i = 1; i <= 10; ++i) {
        std::cout << n << " x " << i << " = " << n * i << std::endl;
    }

    return 0;
}
